{
  "name": "mongodb-orm",
  "version": "1.2.0",
  "repository": "git@github.com:wolframdeus/mongo-orm.git",
  "homepage": "https://github.com/wolframdeus/mongo-orm",
  "bugs": "https://github.com/wolframdeus/mongo-orm/issues",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "mongodb",
    "mongo",
    "orm",
    "typescript"
  ],
  "scripts": {
    "build": "gulp",
    "commit": "git-cz",
    "dev": "ts-node-dev --transpile-only test.ts",
    "lint": "eslint src/**",
    "prepare": "install-peers",
    "postrelease": "npm-run-all release:*",
    "prepublishOnly": "yarn build",
    "release:tags": "git push --follow-tags origin master & yarn publish --non-interactive",
    "release:github": "dotenv -e .env -- conventional-github-releaser -p eslint",
    "release": "standard-version -a",
    "test": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "commitizen": "^4.2.4",
    "conventional-github-releaser": "^3.1.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-minify": "^3.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.0"
  },
  "peerDependencies": {
    "mongodb": "^4.2.1",
    "reflect-metadata": "^0.1.13"
  }
}
